<view><scroll-view class="wxaSortPickerList" style="{{'height:'+(scrollHeight+'px')+';'}}" scroll-y="true" scroll-into-view="{{wxaSortPickerData.wxaSortPickertoView}}"><block a:for="{{wxaSortPickerData.textData}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.textArray.length>0}}"><block><view class="wxaSortPickerTag" id="{{item.tag}}">{{item.tag}}</view><view class="wxaSortPickerItem-box"><block a:for="{{item.textArray}}" a:for-item="child" a:for-index="inde" a:key="inde"><block><view class="wxaSortPickerItem" data-text="{{child.t_nickname}}" data-value="{{child.t_id}}" data-image="{{child.photo}}" data-event-opts="{{[['tap',[['wxaSortPickerItemTap',[inde,'$0','$1','$2','$3','$4'],[[['wxaSortPickerData.textData','',index],['textArray','',inde,'t_nickname']],[['wxaSortPickerData.textData','',index],['textArray','',inde,'t_id']],[['wxaSortPickerData.textData','',index],['textArray','',inde,'photo']],[['wxaSortPickerData.textData','',index,'tag']],[['wxaSortPickerData.textData','',index],['textArray','',inde]]]]]]]}}" onTap="__e"><label class="{{(('_span')+' '+(child.is==1?'sp act':'sp'))}}"></label>{{''+child.t_nickname+''}}</view></block></block></view></block></block></block></scroll-view><scroll-view class="wxaSortPickerTemTags" style="width:20px;"><block a:for="{{charsABC}}" a:for-item="item" a:for-index="idx" a:key="idx"><block><view class="wxaSortPickerTemTag" data-tag="{{item}}" data-event-opts="{{[['tap',[['wxaSortPickerTemTagTap',['$event']]]]]}}" onTap="__e">{{item}}</view></block></block></scroll-view></view>