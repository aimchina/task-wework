<view class="navTabBox"><view class="longTab"><scroll-view style="white-space:nowrap;display:flex;" scroll-x="true" scroll-with-animation scroll-into-view="{{toView}}"><block a:for="{{tabTitle}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('longItem')+' '+(index===tabClick?'click':''))}}" data-index="{{index}}" id="{{'id'+index}}" data-event-opts="{{[['tap',[['longClick',[index,'$0'],[[['tabTitle','',index,'id']]]]]]]}}" onTap="__e">{{item.name}}</view></block><view class="underlineBox" style="{{('transform:translateX('+isLeft+'px);')}}"><view class="underline"></view></view></scroll-view></view></view>