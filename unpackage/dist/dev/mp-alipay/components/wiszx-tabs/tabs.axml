<view class="tabs"><scroll-view class="active-switch" scroll-x scroll-into-view="{{id}}" scroll-with-animation><view class="switch-container"><block a:for="{{TabList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{((('active-item')+' '+(currentTab==index&&'focus'))+' '+(TabList.length==3&&'fix'))}}" id="{{'tab_'+index}}" data-event-opts="{{[['tap',[['tabChange',[index]]]]]}}" onTap="__e"><view class="item">{{item.title}}</view></view></block><view class="{{(('focus-line')+' '+(TabList.length==3&&'fix'))}}" style="{{'transform:'+(transformX)+';'}}"></view></view></scroll-view><view data-event-opts="{{[['touchStart',[['touchstart',['$event']]]],['touchEnd',[['touchend',['$event']]]]]}}" class="tab-pane-view" onTouchStart="__e" onTouchEnd="__e"><view class="tab-pane-group" style="{{'transform:'+(transformXx)+';'}}"><slot></slot></view></view></view>