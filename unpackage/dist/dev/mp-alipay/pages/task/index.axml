<view class="content"><block a:if="{{isShow_loading}}"><view class="box-loading"><loading13 vue-id="1" onVueInit="__l"></loading13></view></block><block a:if="{{!isShow_loading}}"><view class="act"><view data-event-opts="{{[['tap',[['select',['$event']]]]]}}" class="btn _div" onTap="__e"><label class="_span">{{msg}}</label><image style="width:40rpx;height:40rpx;margin-bottom:-8rpx;" src="{{src}}" mode></image></view><view class="{{(((hide)?'list':'')+' '+((show)?'list2':''))}}"><view class="_ul"><block a:for="{{lists}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['clk',[index,'$0','$1'],[[['lists','',index,'id']],[['lists','',index,'name']]]]]]]}}" class="{{(('_li')+' '+(currentid==index?'active':''))}}" onTap="__e">{{item.name}}<label class="_span"></label></view></block></view></view></view></block><view class="xiansuo _div"><block a:if="{{missionlists.length==0}}"><view class="show">暂无记录</view></block><view class="_ul"><block a:for="{{missionlists}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['todetail',['$0'],[[['missionlists','',index,'t_id']]]]]]]}}" class="{{(('_li')+' '+(act==index?'activeLi':''))}}" onTap="__e"><view class="left_btn"><label data-event-opts="{{[['tap',[['del_mission',['$0',index],[[['missionlists','',index,'t_id']]]]]]]}}" catchTap="__e" class="_span"></label></view><view class="right_btn"><label style="display:block;box-sizing:border-box;padding-bottom:10rpx;width:100%;font-size:28rpx;" class="_span">{{item.t_taskContent}}</label><label style="font-size:28rpx;color:#B0B0B0;width:100%;" class="_span">{{item.t_taskEndTime}}</label><block a:if="{{item.guoqi}}"><label style="font-size:28rpx;color:red;width:100%;" class="_span">{{"已过期"+item.guoqi}}</label></block></view></view></block></view></view><view hidden="{{!(isShow)}}"><uni-load-more vue-id="2" status="{{status}}" onVueInit="__l"></uni-load-more></view></view>