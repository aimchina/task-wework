
page {
	background: #F3F3F3;
}
.xiansuo {
	width: 100%;
	margin-top: 62rpx;
	position: relative;
}
.xiansuo ._ul ._li {
	width: 100%;
	/* height: 400upx; */
	padding: 20rpx 30rpx;
	box-sizing: border-box;
	border-bottom: 1px solid #f3f3f3;
	background: #fff;
	overflow: hidden;
	/* position: relative; */
	/* z-index: 1; */
}
.xiansuo ._ul ._li ._span {
	/* width: 100%; */
	float: left;
	margin: 6rpx 0;
	/* height: 40upx; */
	line-height: 40rpx;
}
.xiansuo ._ul ._li .left_btn {
	float: left;
	width: 10%upx;
	/* height: 100upx; */
}
.xiansuo ._ul ._li .left_btn ._span {
	width: 40rpx;
	height: 40rpx;
	border-radius: 8rpx;
	border: 2rpx solid #CCCCCC;
	display: block;
}
.xiansuo ._ul ._li .right_btn {
	float: right;
	width: 90%;
}
.sp1 {
	/* font-weight: bold; */
	color: #000;
}
.sp2 {
	font-size: 30rpx;
	color: #b0b0b0;
}
.act {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 99999;
}
.btn {
	width: 100%;
	height: 60rpx;
	background: #FFFFFF;
	text-align: center;
	font-size: 26rpx;
	border-bottom: 2rpx solid #f3f3f3;
	line-height: 60rpx;
}
.list {
	width: 100%;
	height: 290rpx;
	-webkit-transition: all .5s ease;
	transition: all .5s ease;
	overflow: hidden;
	font-size: 28rpx;
	background: #FFFFFF;
	position: relative;
	z-index: 22;
}
.list2 {
	width: 100%;
	height: 0;
	-webkit-transition: all .5s ease;
	transition: all .5s ease;
	overflow: hidden;
	font-size: 28rpx;
	background: #FFFFFF;
	position: relative;
	z-index: 22;
}
.list ._ul ._li,
.list2 ._ul ._li {
	width: 100%;
	height: 70rpx;
	border-bottom: 2rpx solid #f3f3f3;
	text-align: left;
	padding: 0 20rpx;
	box-sizing: border-box;
	line-height: 70rpx;
	font-size: 26rpx;
	z-index: 999999999 !important;
	background: #FFFFFF;
}
.list ._ul ._li ._span,
.list2 ._ul ._li ._span {
	display: block;
	width: 30rpx;
	height: 30rpx;
	float: right;
	border: 2rpx solid #cecece;
	border-radius: 50%;
	margin-top: 20rpx;
}
.list ._ul .active ._span:after,
.list2 ._ul .active ._span:after {
	content: "";
	display: block;
	width: 20rpx;
	height: 20rpx;
	background: rgb(74, 119, 176);
	border-radius: 50%;
	margin: 6rpx;
}
.list ._ul ._li ._span:after,
.list2 ._ul ._li ._span:after {
	content: "";
	display: block;
	width: 20rpx;
	height: 20rpx;
	/* background: rgb(74,119,176); */
	border-radius: 50%;
	margin: 6rpx;
}
.show {
	width: 100%;
	/* height: 100%; */
	text-align: center;
	padding: 200rpx 0;
	font-size: 28rpx;
}
.box-loading {
	position: fixed;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 1000rpx;
	border: 1px solid rgba(255, 255, 255, 0.1);
	border-radius: 10rpx;
	/* margin-top: 30upx; */
	background: #fff;
	/* flex: 0 0 600upx; */
	/* background: #040038; */
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	overflow: hidden;
	z-index: 99999999999;
}
.activeLi {
	background: #F3F3F3 !important;
}

