
input,
textarea,
button,
._select {
	outline: none;
	resize: none;
	border: 0 none;
	color: #555;
	font-family: "Arial", "Microsoft YaHei";
	font-size: 28rpx;
}
page {
	background-color: #F3F3F3;
}
.kong {
	width: 100%;
	height: 20rpx;
	background: #F3F3F3;
}
.box2 {
	background: #fff;
}
.box4 {
	background: #fff;
}
.lab {
	width: 100%;
	height: 80rpx;
	overflow: hidden;
	padding: 0 0 0 20rpx;
	box-sizing: border-box;
	border-bottom: 2rpx solid #F3F3F3;
}
.lab1 {
	float: left;
	width: 24%;
	height: 80rpx;
	text-align: left;
	font-size: 28rpx;
	line-height: 80rpx;
}
.lab2 {
	float: right;
	width: 75%;
	overflow: hidden;
}
.select input {
	width: 460rpx;
	height: 100%;
	float: left;
	text-align: right;
	padding: 12rpx 0;
	box-sizing: border-box;
	color: #000;
}
.select .right {
	float: right;
	padding: 20rpx;
	box-sizing: border-box;
}
.select .right image {
	width: 40rpx;
	height: 40rpx;
}
.box1 {
	width: 100%;
	padding: 20rpx 20rpx;
	box-sizing: border-box;
	background: #FFFFFF;
}
.box1 .title {
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.box1 .title ._span {
	font-size: 28rpx;
}
.content {
	width: 100%;
	padding: 10rpx;
	box-sizing: border-box;
}
.text_box {
	width: 100%;
	/* height: 100%; */
	padding: 10rpx;
	box-sizing: border-box;
	outline: 0;
	font-size: 26rpx;
}
.footer {
	width: 100%;
	margin-top: 10rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
}
.footer ._span {
	font-size: 26rpx;
}
.footer ._span:nth-child(1) {
	margin-right: 30rpx;
}
.box3 {
	width: 100%;
	padding: 10rpx 0;
	box-sizing: border-box;
	background: #FFFFFF;
}
.box3 .reply {
	width: 100%;
	height: 60rpx;
	padding: 0 20rpx;
	box-sizing: border-box;
	border-bottom: 2rpx solid #F3F3F3;
	line-height: 60rpx;
	font-size: 28rpx;
}

/* .box3 .reply span {
	
} */
.reply_content {
	width: 100%;
	/* height: 80upx; */
	position: fixed;
	padding: 10rpx 20rpx;
	box-sizing: border-box;
	border-top: 2rpx solid #CCCCCC;
	bottom: 0;
	left: 0;
	overflow: hidden;
	background: #fff;
}
.reply_content ._span {
	float: left;
}
.con {
	width: 686rpx;
	margin: 0 auto;
}

/* .reply_content span:nth-child(1) {
	width: 40upx;
	height: 100%;
	margin-right: 80upx;
} */
.reply_content ._span:nth-child(1) {
	width: 540rpx;
	margin-right: 6rpx;
	/* height: 80upx;
	border-radius: 8upx; */
}
.reply_content ._span:nth-child(2) {
	/* float: right; */
	width: 140rpx;
	height: 72rpx;
	background: #0081FF;
	color: #FFFFFF;
	font-size: 24rpx;
	text-align: center;
	line-height: 72rpx;
	/* border-radius: 10upx; */
	border-radius: 10rpx;
}
.reply_content ._span:nth-child(1) textarea {
	border: 2rpx solid #CCCCCC;
	padding: 10rpx 8rpx;
	height: 70rpx;
	border-radius: 10rpx;
	line-height: 46rpx;
	width: 100%;
	box-sizing: border-box;
}
.sp {
	float: left !important;
	width: 460rpx;
	height: 80rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	padding: 10rpx 0;
	box-sizing: border-box;
	text-align: right;
}
.sp ._span {
	/* float: right; */
	/* height: 100%; */
	text-align: center;
	/* line-height: 60upx; */
	font-size: 26rpx;
	box-sizing: border-box;
	/* margin-right: 10upx; */
}

/* .sp span image {
	width: 40upx;
	height: 40upx;
	border-radius: 50%;
} */
.iphoneX {
	padding-bottom: env(safe-area-inset-bottom);
	/* padding-bottom:50upx; */
}
.index {
	background: #F8F8F8;
}

/* .list-box{
	padding: 20upx 0;
} */
.container_of_slide {
	width: 100%;
	overflow: hidden;
}
.slide_list {
	-webkit-transition: all 100ms;
	transition: all 100ms;
	-webkit-transition-timing-function: ease-out;
	        transition-timing-function: ease-out;
	min-width: 200%;
	/* height: 160upx; */
	border-bottom: 2rpx solid #F3F3F3;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: stretch;
	-webkit-align-items: stretch;
	        align-items: stretch;
}
.now-message-info {
	box-sizing: border-box;
	/* display: flex; */
	/* align-items: center; */
	/* justify-content: space-between; */
	font-size: 16px;
	clear: both;
	/* height: 160upx; */
	/* padding: 0 30upx; */
	width: 100%;
	/* margin-bottom: 20upx; */
	background: #FFFFFF;
	padding: 10rpx 20rpx;
	/* box-sizing: border-box; */
}
.now-message-info,
.group-btn {
	float: left;
}
.group-btn {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	/* height: in; */
	min-width: 100rpx;
}
.group-btn .btn-div {
	/* height: 160upx; */
	height: 100%;
	color: #fff;
	text-align: center;
	padding: 0 50rpx;
	font-size: 34rpx;
	/* line-height: 160upx; */
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	display: -webkit-flex;
}
.group-btn .top {
	background-color: #c4c7cd;
}
.group-btn .removeM {
	background-color: #ff3b32;
}
.icon-circle {
	background: #3396fb;
	border-radius: 100%;
	width: 100rpx;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	color: #FFFFFF;
	font-weight: bold;
	font-size: 20px;
	float: left;
}
.list-right {
	width: 100%;
	float: left;
	/* margin-left: 25upx; */
	margin-right: 30rpx;
	overflow: hidden;
}
.list-right-1 {
	float: left;
	color: #000;
	overflow: hidden;
	width: 100%;
	/* padding: 10upx; */
	box-sizing: border-box;
	font-size: 24rpx;
}

/* .titlebox1,.titlebox2{
	width: 750upx;
} */
.list-title {
	width: 350rpx;
	line-height: 1.5;
	float: left;
	overflow: hidden;
	margin-bottom: 10rpx;
	color: #000000;
	font-size: 28rpx;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	overflow: hidden;
}
.list-detail {
	width: 350rpx;
	font-size: 24rpx;
	float: right;
	color: #000;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	overflow: hidden;
	text-align: right;
}
.button-box {
	box-sizing: border-box;
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	z-index: 998;
	background: #F8F8F8;
}
.btn-sub {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	align-items: center;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	float: left;
	width: 100%;
	height: 100rpx;
	background: #F8F8F8;
	color: #7fb2ff;
}
.btn-plusempty {
	width: 110rpx;
	height: 110rpx;
	background: #007bfa;
	position: fixed;
	bottom: 50rpx;
	right: 20rpx;
	border-radius: 100%;
	overflow: hidden;
	text-align: center;
	line-height: 110rpx;
}
.empty {
	color: #999999;
}
.plusempty-img {
	width: 50rpx;
	height: 50rpx;
	margin-top: 30rpx;
}
.scan-box {
	display: block;
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background-color: rgba(0, 0, 0, 0.3);
	z-index: 99999;
}
.scan-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	position: relative;
	margin: 0 auto;
	width: 80%;
	height: 100%;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	align-items: center;
	box-sizing: border-box;
	opacity: 1;
}
.scan-content {
	position: relative;
	width: 400rpx;
	height: 500rpx;
	background: #FFFFFF;
	border-radius: 20rpx;
}
.scan-icon {
	position: absolute;
	top: -50rpx;
	left: 150rpx;
	width: 100rpx;
	height: 100rpx;
	border-radius: 100%;
	box-sizing: border-box;
	background: #FFFFFF;
	padding: 20rpx;
}
.scan-icon-img {
	width: 100%;
	height: 100%;
}
.scan-text {
	position: absolute;
	bottom: 40rpx;
	left: 0;
	width: 100%;
	text-align: center;
	font-size: 14px;
}
.scan-share {
	width: 100%;
	height: 100%;
}
.scan-img {
	width: 300rpx;
	height: 300rpx;
	margin: auto;
	display: block;
	position: absolute;
	top: 80rpx;
	left: 50rpx;
	z-index: 99;
}
.scan-btn {
	top: -30rpx;
	left: auto;
	right: -30rpx;
	bottom: auto;
	position: absolute;
	width: 64rpx;
	height: 64rpx;
	border-radius: 50%;
	z-index: 99999;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.uni-list-cell-hover {
	background-color: #eeeeee;
}
.bottom-btn-hover {
	background: #0564c7 !important;
}
.box-loading2 {
	position: fixed;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 100%;
	/* border: 1px solid rgba(255, 255, 255, 0.1); */
	border-radius: 10rpx;
	margin-top: 30rpx;
	/* flex: 0 0 600upx; */
	/* background: #040038; */
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	overflow: hidden;
}

