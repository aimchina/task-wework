}
.select_radio {
	position: absolute;
	top: 70rpx;
	left: 0;
	right: 0;
	bottom: 0;
	overflow: auto;
}
.select_radio .vw {
	padding: 20rpx 40rpx;
	line-height: 40rpx;
	/* color: #555; */
