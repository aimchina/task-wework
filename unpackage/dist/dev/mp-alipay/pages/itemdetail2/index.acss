}
.change_title{
	width: 100%;
	height: 180rpx;
	overflow: hidden;
	-webkit-transition: all .4s;
	-moz-transition: all .4s;
	margin-top: 10rpx;
}
.change_title .ipt,.change_title2 .ipt{
	width: 90%;
	margin: 0 auto;
	height: 80rpx;
}
.change_title .ipt input,.change_title2 .ipt input{
	width: 100%;
	height: 100%;
	background: #fff;
	padding:16rpx 20rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	/* line-height: 60upx; */
}
.change_title .ipt textarea,.change_title2 .ipt textarea{
	width: 100%;
	height: 100%;
	background: #fff;
	padding:6rpx 20rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	/* line-height: 60upx; */
}
.change_title .btn,.change_title2 .btn{
	width: 90%;
	margin: 30rpx auto 0;
	height: 50rpx;
	overflow: hidden;
}
.change_title .btn ._span,.change_title2 .btn ._span{
	float: right;
	width: 120rpx;
	height: 50rpx;
}
.change_title .btn .sp2,.change_title2 .btn .sp2{
	background: rgb(0,162,232);
	color: #fff;
	font-size: 26rpx;
	text-align: center;
	line-height:50rpx;
	margin-left: 20rpx;
}
.change_title .btn .sp1,.change_title2 .btn .sp1{
	/* background: blue; */
	color: #ccc;
	font-size: 26rpx;
	text-align: center;
	line-height: 50rpx;
	/* margin-left: 20upx; */
	border: 2rpx solid #CCCCCC;
}
.change_title2{
	width: 100%;
	height: 0rpx;
	overflow: hidden;
	-webkit-transition: all 0s;
	-moz-transition: all 0s;
	background: #F3F3F3;
}
.change_title3{
	width: 100%;
	height: 160rpx;
	overflow: hidden;
	-webkit-transition: all .4s;
	-moz-transition: all .4s;
	/* margin-top: 10upx; */
	background: #F3F3F3;
	padding: 10rpx 0;
	box-sizing: border-box;
}
.change_title3 .ipt,.change_title4 .ipt{
	width: 90%;
	margin: 0 auto 0;
	height: 60rpx;
	padding: 10rpx 0;
	box-sizing: border-box;
}
.change_title3 .ipt textarea,.change_title4 .ipt textarea{
	width: 100%;
	height: 60rpx;
	background: #fff;
	padding:6rpx 20rpx;
