.list {
	width: 100%;
	/* height: 90%; */
	padding: 0 30rpx;
	padding-bottom: 10rpx;
	box-sizing: border-box;
	overflow: hidden;
	position: relative;
	overflow-x: hidden;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}
.list-item {
	width: 100%;
	list-style: none;
	background-color: #fff;
	font-size: 30rpx;
	margin: 10rpx 0;
	box-sizing: border-box;
	padding: 20rpx;
	overflow: hidden;
	border-radius: 10rpx;
	word-wrap: break-word;
	position: relative;
}
.list-item .box-loading {
	width: 40rpx;
	height: 40rpx;
	position: absolute;
	top: 50%;
	margin-top: -20rpx;
	right: 20rpx;
	/* margin-top: -15upx; */
}
.list_add,.list_add2 {
	width: 100%;
	height: 80rpx;
	box-sizing: border-box !important;
	text-align: center;
	line-height: 80rpx;
	background: #F3F3F3;
	color: #000;
	font-size: 28rpx;
	padding: 0 40rpx;
	position: absolute;
	bottom: 0;
	left: 0;
	box-sizing: border-box;
}
.list_add2{
	top: 0;
}
.btn {
	width: 100%;
	background: #F3F3F3;
	z-index: 99;
}
.active_add {
	width: 100%;
	height: 140rpx;
	padding: 0 30rpx !important;
	box-sizing: border-box !important;
	text-align: center;
	line-height: 80rpx;
	background: #F3F3F3;
	color: #B0B0B0;
	font-size: 28rpx;
	/* padding: 0 40upx; */
	position: absolute;
	bottom: 0;
	left: 0;
	box-sizing: border-box;
	overflow: hidden;
}
.active_add .act_btn {
	width: 100%;
	height: 100%;
	background: #F3F3F3;
	padding: 8rpx 0 0;
	box-sizing: border-box;
}
.ipt textarea {
	width: 100%;
	height: 70rpx;
	background: #FFFFFF;
	padding: 10rpx;
	box-sizing: border-box;
	text-align: left;
	color: #000;
}
.ipt .btn_left {
	float: left;
	width: 50%;
	height: 60rpx;
	padding: 0 40rpx;
	line-height: 60rpx;
	box-sizing: border-box;
	text-align: left;
	background: #F3F3F3;
	color: red;
}
.ipt .btn_right {
	float: right;
	width: 50%;
	height: 60rpx;
	line-height: 60rpx;
	padding: 0 40rpx;
	box-sizing: border-box;
	text-align: right;
	background: #F3F3F3;
	color: red;
}
.box-loading2 {
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 1200rpx;
	border: 1px solid rgba(255, 255, 255, 0.1);
	border-radius: 10rpx;
	margin-top: 30rpx;
	/* flex: 0 0 600upx; */
	/* background: #040038; */
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	overflow: hidden;
	background: #fff;
}
.fix2{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: rgba(0,0,0,0.4);
	z-index: 999;
}
.fixed_select {
	width: 100%;
	height: 32%;
	max-width: 1280rpx;
	position: fixed;
	bottom: -40%;
	background: white;
	z-index: 1000;
	-webkit-transition: all .4s;
	-moz-transition: all .4s;
}
