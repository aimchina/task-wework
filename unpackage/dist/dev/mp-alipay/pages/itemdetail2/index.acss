	box-sizing: border-box;
	font-size: 28rpx;
}
.change_title3 .btn2,.change_title4 .btn2{
	width: 90%;
	margin: 20rpx auto 0;
	height: 60rpx;
	overflow: hidden;
	padding: 0 30rpx;
}
.change_title3 .btn2 ._span,.change_title4 .btn2 ._span{
	width: 100rpx;
	height: 50rpx;
}
.change_title3 .btn2 .sp2,.change_title4 .btn2 .sp2{
	/* background: rgb(0,162,232); */
	color: red;
	float: right;
	font-size: 30rpx;
	text-align: center;
	line-height:50rpx;
	margin-left: 20rpx;
	height: 50rpx;
}
.change_title3 .btn2 .sp1,.change_title4 .btn2 .sp1{
