
page{
	height:100%;
	width:100%;
	/*top:0;
	left:0;*/
	overflow:hidden;
	-webkit-overflow-scrolling: touch;
}
.sortable-chosen {
	opacity: 1 !important;
}
.ghostClass {
	opacity: 0 !important;
}
.box1 {
	width: 100%;
	height: 93%;
	background: #fff;
	box-sizing: border-box;
	padding: 30rpx  40rpx 0;
}
/* 搜索列表 */
.list-item {
	width: 100%;
	list-style: none;
	background-color: #fff;
	font-size: 30rpx;
	/* margin: 10upx 0; */
	box-sizing: border-box;
	padding: 20rpx;
	overflow: hidden;
	border-radius: 10rpx;
	word-wrap: break-word;
	position: relative;
	border-bottom: 2rpx solid #F3F3F3;
}
.addbox1{
	width: 100%;
	height: 100%;
	background: #fff;
	box-sizing: border-box;
	padding:40rpx;
}
.content {
	width: 100%;
	height: 100%;
	background: #F3F3F3;
	/* border-radius: 10upx; */
	box-sizing: border-box;
	overflow: hidden;
	padding: 10rpx 0;
	position: relative;
	position: relative;
}
.content2 {
	width: 100%;
	height: 100%;
	/* background: #F3F3F3; */
	/* border-radius: 10upx; */
	box-sizing: border-box;
	overflow: hidden;
	padding: 10rpx 0;
	position: relative;
	position: relative;
}
.content ._span {
	display: block;
	width: 100%;
	/* padding: 0 30upx 0; */
	/* padding-left: 30upx; */
	box-sizing: border-box;
	font-size: 34rpx;
	/* line-height: 80upx; */
	overflow: hidden;
}
.spa {
	padding-left: 30rpx;
	line-height: 80rpx;
}
.content ._span .sp {
	float: right;
	width: 80rpx;
	height: 60rpx;
	/* margin-top: 10upx; */
	margin: 10rpx 30rpx 0 0;
}
