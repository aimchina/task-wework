<view class="box"><view class="cu-form-group"><view class="title">项目名称</view><input placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['','title','$event',[]]]]]]}}" value="{{title}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['goAddMember',['$event']]]]]}}" class="cu-form-group" bindtap="__e"><view class="title">项目成员</view><view class="img"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="__i0__"><label class="_span"><image src="{{item}}" mode></image></label></block></view></view><view class="cu-form-group"><view class="title">设置开始时间</view><picker mode="date" value="{{starttime}}" start="2000-09-01" end="2030-09-01" data-event-opts="{{[['change',[['startChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+starttime+''}}</view></picker></view><view class="cu-form-group"><view class="title">设置截止时间</view><picker mode="date" value="{{endtime}}" start="2000-09-01" end="2030-09-01" data-event-opts="{{[['change',[['endChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+endtime+''}}</view></picker></view><view data-event-opts="{{[['tap',[['gomouldlist',['$event']]]]]}}" class="cu-form-group" bindtap="__e"><view class="title">项目模板</view><view class="mould_name">{{mould_name}}</view></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="cu-btn bg-blue lg" bindtap="__e">完成</button></view></view>